{
  "name": "IRIS-MD",
  "version": "2.0.9",
  "description": "Your Whatsapp Assistant",
  "keywords": ["whatsapp", "bot", "md", "multi-device"],
  "repository": "https://github.com/V-E-N-O-X/v2",
  "stack": "container",
  "env": {
    "SESSION_ID": {
      "description": "paste Your Session ID",
      "required": true
    },
    "PREFIX": {
      "description": "Enter Prefix For Commands",
      "required": true,
      "value": "."
    },
     "OWNER_NAME": {
      "description": "Put your OwnerName here",
      "required": false,
      "value": "VenoxSenpai"
    },
     "BOT_NAME": {
      "description": "Put your BotName here",
      "required": false,
      "value": "Iris-Md"
    },
    "SUDO": {
      "description": "Enter Numbers With Country Code and Without (+)",
      "required": true,
      "value": "8801975492880"
    }
  },
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git"
    }
  ]
}
